function [ outputSTN, outputValueList  ] = checkTimings( inputSTN, inputValueList )

    [index, value] = min(inputValueList);
    
    try
        outputSTN = insertValue(inputSTN, index, value);
    catch err
        
        if ( strcmp( err.identifier, 'STN:NonZeroDiag'))
            
        else
            rethrow(err);
        end
    end
end

